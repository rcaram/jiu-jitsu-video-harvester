
# YouTube API key - required for video search functionality
YOUTUBE_API_KEY=your_youtube_api_key_here

# Clerk authentication - required for user login
# Get this from your Clerk dashboard: https://dashboard.clerk.com/last-active?path=api-keys
# IMPORTANT: Create a .env file with this key before running the app
# Must start with 'pk_' (e.g., pk_test_abc123... or pk_live_abc123...)
VITE_CLERK_PUBLISHABLE_KEY=pk_test_xxxxxxxxxxxxxxxxxxxxxxxx

# Port for the server (optional)
PORT=3001
